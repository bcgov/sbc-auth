{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/affiliation",
  "type": "object",
  "title": "Affiliation",
  "required": [
    "businessIdentifier"
  ],
  "properties": {
    "businessIdentifier": {
      "$id": "#/properties/businessIdentifier",
      "type": "string",
      "title": "Business Identifier",
      "default": "",
      "examples": [
        "CP1234567"
      ],
      "pattern": "^(.*)$"
    },
    "passCode": {
      "$id": "#/properties/passCode",
      "type": "string",
      "title": "Passcode",
      "default": "",
      "examples": [
        "12345"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "Email",
      "default": "",
      "examples": [
        "test@test.com"
      ],
      "pattern": "^(.*)$"
    },
    "phone": {
      "$id": "#/properties/phone",
      "type": "string",
      "title": "Phone",
      "default": "",
      "examples": [
        "1112223333"
      ],
      "pattern": "^(.*)$"
    },
    "certifiedByName": {
      "$id": "#/properties/certifiedByName",
      "type": "string",
      "title": "Certified By Name",
      "default": "",
      "examples": [
        "Last, First"
      ],
      "pattern": "^(.*)$"
    }
  }
}
  