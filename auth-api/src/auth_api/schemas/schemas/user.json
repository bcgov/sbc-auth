{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/user",
    "type": "object",
    "title": "User",
    "additionalProperties": false,
    "properties": {
      "firstName": {
        "$id": "#/properties/firstName",
        "type": "string",
        "title": "First Name",
        "default": "",
        "examples": [
          "John"
        ],
        "pattern": "^(.*)$"
      },
      "lastName": {
        "$id": "#/properties/lastName",
        "type": "string",
        "title": "Last Name",
        "default": "",
        "examples": [
          "Doe"
        ],
        "pattern": "^(.*)$"
      },
      "isLogin": {
        "$id": "#/properties/isLogin",
        "type": "boolean",
        "title": "Is Login",
        "default": "",
        "examples": [
          true, false
        ]
      }
    }
  }
