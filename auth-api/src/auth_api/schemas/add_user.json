{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Add User Schema",
  "required": [
    "username",
    "password"
  ],
  "properties": {
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "The Username Schema",
      "default": "",
      "examples": [
        "test11"
      ],
      "pattern": "^(.*)$"
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The Password Schema",
      "default": "",
      "examples": [
        "1111"
      ],
      "pattern": "^(.*)$"
    },
    "firstname": {
      "$id": "#/properties/firstname",
      "type": "string",
      "title": "The Firstname Schema",
      "default": "",
      "examples": [
        "111"
      ],
      "pattern": "^(.*)$"
    },
    "lastname": {
      "$id": "#/properties/lastname",
      "type": "string",
      "title": "The Lastname Schema",
      "default": "",
      "examples": [
        "test"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        "test11@gov.bc.ca"
      ],
      "pattern": "^(.*)$"
    },
    "enabled": {
      "$id": "#/properties/enabled",
      "type": "boolean",
      "title": "The Enabled Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "user_type": {
      "$id": "#/properties/user_type",
      "type": "array",
      "title": "The User_type Schema",
      "items": {
        "$id": "#/properties/user_type/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "/test",
          "/basic/editor"
        ],
        "pattern": "^(.*)$"
      }
    },
    "corp_type": {
      "$id": "#/properties/corp_type",
      "type": "string",
      "title": "The Corp_type Schema",
      "default": "",
      "examples": [
        "CP"
      ],
      "pattern": "^(.*)$"
    },
    "source": {
      "$id": "#/properties/source",
      "type": "string",
      "title": "The Source Schema",
      "default": "",
      "examples": [
        "PASSCODE"
      ],
      "pattern": "^(.*)$"
    }
  }
}
